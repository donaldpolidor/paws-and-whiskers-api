{
  "name": "paws-and-whiskers-api",
  "version": "1.0.0",
  "description": "API for dog and cat breeds database - CSE 341 Final Project",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node seed.js",
