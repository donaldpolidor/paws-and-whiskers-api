    "express": "^4.18.2",
    "express-session": "^1.19.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "chai": "^5.2.0",
    "chai-http": "^5.1.2",
    "jest": "^29.7.0",
